-- Loading Rayfield (ensure you're using a proper loader)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create Window
local Window = Rayfield:CreateWindow({
   Name = "üåå Dark Hub XYZ",
   Icon = "flame", -- Lucide icon for sleek visuals
   LoadingTitle = "Connecting to the Void...",
   LoadingSubtitle = "Crafted by Darkon ‚òÅÔ∏è",
   Theme = "Midnight", -- Use a professional theme

   ConfigurationSaving = {
      Enabled = true,
      FolderName = "DarkHubXYZ",
      FileName = "Dark_Hub_Config"
   },

   KeySystem = false,
   KeySettings = {
      Title = "üîê Access Gate",
      Subtitle = "Verification Required",
      Note = "Enter: im not a robot",
      FileName = "darkhub_keyfile",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"im not a robot"}
   }
})

-- TABS
local Home = Window:CreateTab("üè† Home")
local Fun = Window:CreateTab("üòé Troll Scripts")
local Combat = Window:CreateTab("‚öîÔ∏è Combat")
local Exploits = Window:CreateTab("üß™ Game Exploits")
local Auto = Window:CreateTab("ü§ñ Autofarms")
local Misc = Window:CreateTab("üõ†Ô∏è Utilities")

-- HOME SECTION
Home:CreateSection("Quick Launch")
Home:CreateButton({
   Name = "Infinite Jump",
   Callback = function()
      game:GetService("UserInputService").JumpRequest:Connect(function()
         game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
      end)
   end
})

